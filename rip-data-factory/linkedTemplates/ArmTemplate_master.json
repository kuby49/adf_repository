{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "rip-data-factory"
		},
		"DWSynapseAnalytics_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'DWSynapseAnalytics'"
		},
		"DWSynapseAnalytics2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'DWSynapseAnalytics2'"
		},
		"ADLSSink_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://awdlstudrip.dfs.core.windows.net"
		},
		"HttpSource_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/djpmsft/adf-ready-demo/master/moviesDB.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "rip-data-factory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"DWSynapseAnalytics_connectionString": {
						"value": "[parameters('DWSynapseAnalytics_connectionString')]"
					},
					"DWSynapseAnalytics2_connectionString": {
						"value": "[parameters('DWSynapseAnalytics2_connectionString')]"
					},
					"ADLSSink_properties_typeProperties_url": {
						"value": "[parameters('ADLSSink_properties_typeProperties_url')]"
					},
					"HttpSource_properties_typeProperties_url": {
						"value": "[parameters('HttpSource_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}