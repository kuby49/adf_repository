{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "SynapseDataFactory-6zvtj"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/NyC_Curated')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "synapsedatalake",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "Lab3"
				},
				"annotations": [],
				"type": "Parquet",
				"typeProperties": {
					"location": {
						"type": "AzureBlobStorageLocation",
						"container": "nyctaxidata-curated"
					},
					"compressionCodec": "snappy"
				},
				"schema": [
					{
						"name": "VendorID",
						"type": "INT32"
					},
					{
						"name": "PickUpDate",
						"type": "DATE"
					},
					{
						"name": "PickUpDateTime",
						"type": "INT96"
					},
					{
						"name": "DropOffDate",
						"type": "DATE"
					},
					{
						"name": "DropOffDateTime",
						"type": "INT96"
					},
					{
						"name": "PassengerCount",
						"type": "INT32"
					},
					{
						"name": "TripDistance",
						"type": "DOUBLE"
					},
					{
						"name": "PickUpLocationID",
						"type": "INT32"
					},
					{
						"name": "PickUpLocationZone",
						"type": "UTF8"
					},
					{
						"name": "PickUpLocationBorough",
						"type": "UTF8"
					},
					{
						"name": "DropOffLocationID",
						"type": "INT32"
					},
					{
						"name": "DropOffLocationZone",
						"type": "UTF8"
					},
					{
						"name": "DropOffLocationBorough",
						"type": "UTF8"
					},
					{
						"name": "PaymentTypeID",
						"type": "INT32"
					},
					{
						"name": "PaymentTypeDescription",
						"type": "UTF8"
					},
					{
						"name": "FareAmount",
						"type": "DECIMAL",
						"precision": 8,
						"scale": 2
					},
					{
						"name": "ExtraAmount",
						"type": "DECIMAL",
						"precision": 8,
						"scale": 2
					},
					{
						"name": "MTATaxAmount",
						"type": "DECIMAL",
						"precision": 8,
						"scale": 2
					},
					{
						"name": "TipAmount",
						"type": "DECIMAL",
						"precision": 8,
						"scale": 2
					},
					{
						"name": "TollsAmount",
						"type": "DECIMAL",
						"precision": 8,
						"scale": 2
					},
					{
						"name": "ImprovementSurchargeAmount",
						"type": "DECIMAL",
						"precision": 8,
						"scale": 2
					},
					{
						"name": "TotalRideAmount",
						"type": "DECIMAL",
						"precision": 8,
						"scale": 2
					},
					{
						"name": "PickUpYearMonth",
						"type": "UTF8"
					}
				]
			},
			"dependsOn": []
		}
	]
}